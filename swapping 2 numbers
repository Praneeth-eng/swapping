#include<stdio.h>
int main()
{
double first,second,temp;
printf("enter the first number");
scanf("%d",&first);
printf("enter the second number");
scanf("%d",&second);
temp = first;
first = second;
second = temp;
printf("\nafter swapping first number = %2d\n",first);
printf("after swapping second number = %2d",second);
return 0;
}
